<%= form_for(@district) do |f| %>

<div class="field">
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</div>
<div class="field">
  <%= f.label :permalink %><br />
  <%= f.text_field :permalink %>
</div>

<div class="field" id="is_park">
	<%= f.check_box :is_park %>
	<%= f.label :is_park %>
	
</div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
